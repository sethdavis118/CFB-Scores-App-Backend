{
  "name": "capstone-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "Congratulations! You've made it to the Capstone project!",
  "main": "app.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "db:schema": "psql -d cfbodds -f ./src/db/schema.sql",
    "db:seed": "node --env-file=.env ./src/db/seed.js",
    "db:seed-debug": "nodemon --inspect --env-file=.env ./src/db/seed.js",
    "db:reset": "npm run db:schema && npm run db:seed",
    "debug": "node --inspect ./src/queries/games.js"
  },
